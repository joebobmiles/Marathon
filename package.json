{
  "name": "marathon-project-manager",
  "private": true,
  "description": "A web app for using Agile project management with Github.",
  "author": {
    "name": "Joseph R Miles",
    "email": "me@josephrmiles.com",
    "url": "https://josephrmiles.com"
  },
  "license": "MIT",
  "version": "0.1.0",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joebobmiles/Marathon.git"
  },
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "test": "jest",
    "deploy": "npm run build && gh-pages -d public -b master",
    "deploy:ci": "npm run build && gh-pages -d public -b master -r https://$GH_TOKEN@github.com/joebobmiles/Marathon.git"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.8.0",
    "@babel/core": "^7.5.5",
    "gatsby": "^4.25.7",
    "gatsby-plugin-sass": "^2.1.3",
    "gatsby-plugin-typescript": "^2.1.2",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "typescript": "^3.5.3",
    "webpack": "^4.36.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.2.2",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.1",
    "@types/jsdom": "^16.2.1",
    "gatsby-plugin-tslint": "^0.0.2",
    "gh-pages": "^2.2.0",
    "jest": "^26.0.0",
    "jsdom": "^16.2.2",
    "ts-jest": "^25.4.0",
    "tslint": "^5.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      ".+\\.s[ac]ss$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache",
      "<rootDir>.*/public"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ]
  }
}
